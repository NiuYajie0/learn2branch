        {
            "name": "03 - Train GCNN",
            "type": "python",
            "request": "launch",
            "program": "S03_train_gcnn.py",
            "console": "integratedTerminal",
            "args": [
                "setcover", // choices=['setcover', 'cauctions', 'facilities', 'indset']
                "--sampling", "depthK2", // choices: uniform_5, depthK, depthK2
                "--sample_seed", "1",
                "--seeds", "range(0,10)",
            ],
        },
        {
            "name": "04 - Test",
            "type": "python",
            "request": "launch",
            "program": "S04_test.py",
            "console": "integratedTerminal",
            "args": [
                "setcover", // choices=['setcover', 'cauctions', 'facilities', 'indset']
                "--sampling", "depthK2",  // choices: uniform_5, depthK, depthK2
                "--sample_seed", "1",
                "--seeds", "range(0,10)",
